local success, result = pcall(function()
    fireproximityprompt(workspace:WaitForChild("Trophy"):WaitForChild("ProxHold"):WaitForChild("ProximityPrompt"), 1, Enum.KeyCode.E, game.Players.LocalPlayer)
end)

if success then
    print("ProximityPrompt triggered successfully.")
else
    warn("Failed to trigger ProximityPrompt:", result)
end
